{
  "name": "expense-tracker",
  "version": "1.0.0",
  "private": true,
  "description": "A modern expense tracking application with multi-theme support and PWA capabilities",
  "author": "Ketan K",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/Ketan-K/expense-tracker.git"
  },
  "keywords": [
    "expense-tracker",
    "finance",
    "budget",
    "pwa",
    "nextjs",
    "mongodb",
    "multi-theme"
  ],
  "scripts": {
    "dev": "next dev --webpack",
    "build": "next build --webpack",
    "start": "next start",
    "lint": "eslint",
    "lint:fix": "eslint --fix",
    "format": "prettier --write .",
    "icons:generate": "node scripts/generate-icons.mjs",
    "prepare": "husky",
    "dev:acme": "cross-env NEXT_PUBLIC_THEME=acme next dev --webpack",
    "build:acme": "cross-env NEXT_PUBLIC_THEME=acme next build --webpack",
    "start:acme": "cross-env NEXT_PUBLIC_THEME=acme next start",
    "dev:vibe": "cross-env NEXT_PUBLIC_THEME=vibe next dev --webpack",
    "build:vibe": "cross-env NEXT_PUBLIC_THEME=vibe next build --webpack",
    "start:vibe": "cross-env NEXT_PUBLIC_THEME=vibe next start",
    "build:all": "npm run build && npm run build:acme && npm run build:vibe",
    "cap:init": "npx cap init",
    "cap:sync": "npx cap sync",
    "cap:open:android": "npx cap open android"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.11.1",
    "@capacitor-community/contacts": "^7.1.0",
    "@capacitor/android": "^8.0.2",
    "@capacitor/app": "^8.0.0",
    "@capacitor/browser": "^8.0.0",
    "@capacitor/cli": "^7.4.5",
    "@capacitor/core": "^8.0.2",
    "@capacitor/splash-screen": "^8.0.0",
    "@capacitor/status-bar": "^8.0.0",
    "@next/third-parties": "^16.1.6",
    "@supabase/supabase-js": "^2.94.1",
    "bson": "^6.10.4",
    "date-fns": "^4.1.0",
    "dexie": "^4.2.1",
    "dexie-react-hooks": "^4.2.0",
    "framer-motion": "^12.29.2",
    "isomorphic-dompurify": "^2.30.0",
    "lucide-react": "^0.563.0",
    "mongodb": "^6.21.0",
    "next": "16.1.6",
    "next-auth": "^5.0.0-beta.30",
    "next-pwa": "^5.6.0",
    "next-themes": "^0.4.6",
    "pg": "^8.18.0",
    "react": "19.2.3",
    "react-dom": "19.2.3",
    "recharts": "^3.7.0",
    "sonner": "^2.0.7",
    "validator": "^13.15.26",
    "workbox-webpack-plugin": "^7.4.0",
    "workbox-window": "^7.4.0",
    "xlsx": "^0.18.5"
  },
  "devDependencies": {
    "@tailwindcss/postcss": "^4",
    "@types/node": "^20",
    "@types/pg": "^8.16.0",
    "@types/react": "^19",
    "@types/react-dom": "^19",
    "@types/validator": "^13.15.10",
    "cross-env": "^10.1.0",
    "dotenv": "^17.2.3",
    "eslint": "^9",
    "eslint-config-next": "16.1.6",
    "husky": "^9.1.7",
    "lint-staged": "^16.2.7",
    "png-to-ico": "^3.0.1",
    "prettier": "^3.8.1",
    "tailwindcss": "^4",
    "tsx": "^4.21.0",
    "typescript": "^5"
  },
  "lint-staged": {
    "*.{js,jsx,ts,tsx}": [
      "eslint --fix",
      "prettier --write"
    ],
    "*.{json,css,md}": [
      "prettier --write"
    ]
  }
}
